    "import numpy as np\n",
    "\n",
    "def twovet_angle(v,u):\n",
    "    #sa√≠da angulo theta (graus)\n",
    "    theta = np.arccos((v[0]*u[0]+v[1]*u[1])/(np.sqrt(u[0]**2+u[1]**2)*np.sqrt(v[0]**2+v[1]**2)))*180/np.pi\n",
    "    return theta"
   ]
  }
